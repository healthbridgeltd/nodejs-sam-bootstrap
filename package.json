{
  "name": "nodejs-sam-bootstrap",
  "version": "1.0.0",
  "description": "Cloneable bootstrap for AWS SAM and NodeJS setup",
  "repository": "https://github.com/healthbridgeltd/nodejs-sam-bootstrap/",
  "author": "Zava",
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "dependencies": {
    "aws-sdk": "2.677.0",
    "source-map-support": "0.5.19"
  },
  "scripts": {
    "lint": "standardx ./src/**/*.ts --env jest | standard-reporter --stylish",
    "lint:fix": "standardx ./src/**/*.ts --env jest --fix",
    "test:watch": "jest --watchAll",
    "test": "jest --coverage",
    "build": "webpack-cli",
    "clean": "rimraf .aws-sam .vscode",
    "prebuild": "rimraf .aws-sam .vscode",
    "prewatch": "rimraf .aws-sam .vscode",
    "watch": "webpack-cli -w"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.51",
    "@types/jest": "25.2.3",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "aws-sam-webpack-plugin": "0.6.0",
    "fork-ts-checker-webpack-plugin": "4.1.4",
    "jest": "25.1.0",
    "recursive-install": "1.4.0",
    "standard-reporter": "1.0.5",
    "standardx": "5.0.0",
    "terser-webpack-plugin": "3.1.0",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "typescript": "4.0.5",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.11"
  }
}
